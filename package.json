{
	"name": "postgis-express",
	"version": "1.0.0",
	"license": "MIT",
	"scripts": {
		"dev": " npm run format && cross-env NODE_ENV=dev nodemon -r tsconfig-paths/register src/index.ts",
		"build": "npm run format && cross-env NODE_ENV=prod tsc && tsc-alias",
		"start": "cross-env NODE_ENV=prod node -r tsconfig-paths/register -r ts-node/register/transpile-only dist/index.js",
		"format": "prettier --write \"**/*.{ts,js,json}\" && eslint src/**/*.ts",
		"test": "jest",
		"prepare": "husky",
		"data:import:dev": "ts-node -r tsconfig-paths/register src/seed.ts",
		"data:import:prod": "node dist/seed.js",
		"worker:email": "ts-node -r tsconfig-paths/register src/workers/email.worker.ts",
		"create:endpoint": "chmod +x create-endpoint.sh && ./create-endpoint.sh"
	},
	"dependencies": {
		"@bull-board/api": "^6.7.7",
		"@bull-board/express": "^6.7.7",
