		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"dayjs": "^1.11.13",
		"dotenv": "^16.4.5",
		"eslint": "^9.13.0",
		"express": "^4.21.2",
		"express-rate-limit": "^7.4.1",
		"express-validator": "^7.2.0",
		"handlebars": "^4.7.8",
		"helmet": "^8.0.0",
		"hpp": "^0.2.3",
		"jsonwebtoken": "^9.0.2",
		"lodash": "^4.17.21",
		"mongoose": "^8.7.2",
		"multer": "^1.4.5-lts.1",
		"nodemailer": "^6.10.0",
		"prettier": "^3.3.3",
		"redis": "^4.7.0",
		"socket.io": "^4.8.0",
		"stripe": "^17.7.0",
